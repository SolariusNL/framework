      "runHostnameCheck": true,
      "enabled": true,
      "limit": {
        "count": 5,
        "frequency": 43200000
      }
    },
    "domains": {
      "enabled": true
    },
    "api-keys": {
      "enabled": true
    },
    "oauth2": {
      "enabled": true
    },
    "dev-profiles": {
      "enabled": true
    },
    "bits": {
      "enabled": true
    },
    "redis": {
      "enabled": true
    },
    "admin-sso": {
      "enabled": false,
      "sso": {
        "clientId": "client_id_here",
        "clientSecret": "client_secret_here",
        "callbackUrl": "http://127.0.0.1:3000/api/admin/sso/callback",
        "authorizationUrl": "https://id.solarius.me/application/o/authorize/",
        "tokenUrl": "https://id.solarius.me/application/o/token/",
        "userInfoUrl": "https://id.solarius.me/application/o/userinfo/"
