  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_VERSION=0.5.2 concurrently \"cd cron && yarn run start\" \"next dev\"",
    "build": "next build",
    "start": "cross-env NEXT_PUBLIC_VERSION=0.5.2 concurrently \"cd cron && yarn run start\" \"next start\"",
    "lint": "next lint",
    "migrate": "npx prisma migrate dev && npx prisma generate",
    "studio": "npx prisma studio",
    "start:prod": "cross-env NEXT_PUBLIC_VERSION=0.5.2 concurrently \"cd cron && yarn run start\" \"next start --port=88\"",
    "generate": "npx prisma generate",
    "seed": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" scripts/seed.ts",
    "ctl": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" scripts/frameworkctl.ts",
    "postinstall": "cp-cli node_modules/.prisma/client/ cron/node_modules/.prisma/client",
    "setup-projects": "cd cron && yarn && cd .. && yarn run generate && yarn && yarn run create-config && yarn run create-config-schema",
    "analyze": "cross-env ANALYZE=true yarn run build",
    "create-config": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" scripts/create-config.ts",
    "create-config-schema": "typescript-json-schema src/types/Configuration.ts Configuration > framework.schema.json"
